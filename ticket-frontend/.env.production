# Ticket Frontend - Production Environment Configuration

# API Gateway Configuration
VITE_API_BASE_URL=http://api-gateway:8080
VITE_API_TIMEOUT=10000

# Service URLs - These are internal Docker network URLs
VITE_USER_SERVICE_URL=http://user-service:8000
VITE_TICKET_SERVICE_URL=http://ticket-service:8001
VITE_FILE_SERVICE_URL=http://file-service:8002
VITE_NOTIFICATION_SERVICE_URL=http://notification-service:8003
VITE_REPORT_SERVICE_URL=http://report-service:8004
VITE_ANALYTICS_SERVICE_URL=http://analytics-service:8005

# Authentication
VITE_AUTH_STORAGE_KEY=auth_token
VITE_AUTH_EXPIRY_DAYS=7

# Features
VITE_ENABLE_SOCKET_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true