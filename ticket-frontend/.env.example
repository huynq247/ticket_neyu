# Ticket Frontend - Configuration Example
# Copy this file to .env and modify as needed

# API Gateway Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_API_TIMEOUT=10000

# Service URLs 
VITE_USER_SERVICE_URL=http://localhost:8000
VITE_TICKET_SERVICE_URL=http://localhost:8001
VITE_FILE_SERVICE_URL=http://localhost:8002
VITE_NOTIFICATION_SERVICE_URL=http://localhost:8003
VITE_REPORT_SERVICE_URL=http://localhost:8004
VITE_ANALYTICS_SERVICE_URL=http://localhost:8005

# Authentication
VITE_AUTH_STORAGE_KEY=auth_token
VITE_AUTH_EXPIRY_DAYS=7

# Features
VITE_ENABLE_SOCKET_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true

# Development Settings
VITE_DEV_SERVER_PORT=3000